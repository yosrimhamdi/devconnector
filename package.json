{
    "name": "devconnector",
    "version": "1.0.0",
    "main": "server.js",
    "scripts": {
        "install": "npm install --prefix client",
        "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
        "start-server": "node server.js",
        "start-client": "npm start --prefix client",
        "build": "npm install && npm run build --prefix client",
        "eslint": "eslint --init"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "history": "^5.0.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "mongoose": "^5.10.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "sharp": "^0.26.1"
    },
    "devDependencies": {
        "concurrently": "^6.5.1",
        "eslint": "^7.17.0"
    },
    "engines": {
        "node": "14.x"
    }
}
